<?xml version="1.0" encoding="UTF-8" ?>
<svMultiPhysicsFile version="0.1">

<GeneralSimulationParameters>
  <Continue_previous_simulation> false </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 3 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 30 </Number_of_time_steps> 
  <Time_step_size> 0.01 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> true </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 10 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 5000 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 
</GeneralSimulationParameters>

<Add_mesh name="msh" > 

  <Mesh_file_path> ./mesh/solid-mesh-complete/mesh-complete.mesh.vtu   </Mesh_file_path>

  <Add_face name="endo">
      <Face_file_path> ./mesh/solid-mesh-complete/mesh-surfaces/wall_inner.vtp </Face_file_path>
  </Add_face>

  <Add_face name="start">
      <Face_file_path> ./mesh/solid-mesh-complete/mesh-surfaces/wall_inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="end">
      <Face_file_path> ./mesh/solid-mesh-complete/mesh-surfaces/wall_outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="epi">
      <Face_file_path> ./mesh/solid-mesh-complete/mesh-surfaces/wall_outer.vtp </Face_file_path>
  </Add_face>

  <Domain_file_path> ./mesh/solid-mesh-complete/domain.dat </Domain_file_path>

   <Fiber_direction_file_path> ./mesh/solid-mesh-complete/fibersCirc.vtu </Fiber_direction_file_path>
   <Fiber_direction_file_path> ./mesh/solid-mesh-complete/fibersLong.vtu </Fiber_direction_file_path>
   <Mesh_scale_factor> 1 </Mesh_scale_factor>

</Add_mesh>

<Add_equation type="struct" >

   <Coupled> true </Coupled>
   <Min_iterations> 1 </Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-3 </Tolerance> 

   <Domain id="1" >
      <Dilational_penalty_model> M94 </Dilational_penalty_model>
      <Density> 1.0 </Density>                           <!-- g/cm^3 -->
      <Elasticity_modulus> 1.0e6 </Elasticity_modulus>   <!-- dyne/cm^2 -->
      <Poisson_ratio> 0.483333 </Poisson_ratio>

      <Constitutive_model type="HolzapfelOgden"> 
         <a> 590.0 </a>                                   <!-- dyne/cm^2 -->
         <b> 8.023 </b>                                   <!-- no units -->
         <a4f> 184720.0 </a4f>
         <b4f> 16.026 </b4f>
         <a4s> 24810.0 </a4s>
         <b4s> 11.12 </b4s>
         <afs> 2160.0 </afs>
         <bfs> 11.436 </bfs>
         <k> 100.0 </k>
      </Constitutive_model> 
  </Domain>

   <Domain id="2" >
      <Dilational_penalty_model> M94 </Dilational_penalty_model>
      <Density> 1.0 </Density>                           <!-- g/cm^3 -->
      <Elasticity_modulus> 1.0e6 </Elasticity_modulus>   <!-- dyne/cm^2 -->
      <Poisson_ratio> 0.483333 </Poisson_ratio>

      <Constitutive_model type="HolzapfelOgden"> 
         <a> 590.0 </a>                                   <!-- dyne/cm^2 -->
         <b> 8.023 </b>                                   <!-- no units -->
         <a4f> 184720.0 </a4f>
         <b4f> 16.026 </b4f>
         <a4s> 24810.0 </a4s>
         <b4s> 11.12 </b4s>
         <afs> 2160.0 </afs>
         <bfs> 11.436 </bfs>
         <k> 100.0 </k>
      </Constitutive_model> 
  </Domain>

   <Add_BC name="start" > 
      <Type> Dirichlet </Type> 
      <Value> 0.0 </Value>
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral>
      <Zero_out_perimeter> false </Zero_out_perimeter>
      <Effective_direction> (0, 0, 1) </Effective_direction>
   </Add_BC> 

   <Add_BC name="end" > 
      <Type> Dirichlet </Type> 
      <Value> 0.0 </Value>
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral>
      <Zero_out_perimeter> false </Zero_out_perimeter>
      <Effective_direction> (0, 0, 1) </Effective_direction>
   </Add_BC> 

   <Add_BC name = "epi" >
      <Type> Robin </Type>
      <Stiffness> 1e4 </Stiffness>
      <Damping> 500 </Damping>
   </Add_BC>

   <Add_BC name = "endo" > 
      <Type> Dir </Type>  
      <Time_dependence> Unsteady </Time_dependence> 
      <Temporal_values_file_path> load.dat </Temporal_values_file_path> 
      <Ramp_function> true </Ramp_function> 
      <Effective_direction> (0, 0, 1) </Effective_direction> 
      <Zero_out_perimeter> false </Zero_out_perimeter>
      <Follower_pressure_load> true </Follower_pressure_load> 
   </Add_BC>

   <LS type="GMRES" >
      <Linear_algebra type="fsils" >
         <Preconditioner> fsils </Preconditioner>
      </Linear_algebra>
      <Max_iterations> 600 </Max_iterations>
      <Tolerance> 1e-12 </Tolerance>
   </LS>

   <Output type="Spatial" >
      <Displacement> true </Displacement>
      <Velocity> true </Velocity>
      <Jacobian> true </Jacobian>
      <Stress> true </Stress>
      <Strain> true </Strain>
      <Fiber_stretch> true </Fiber_stretch>
      <Fiber_stretch_rate> true </Fiber_stretch_rate>
      <Active_tension> true </Active_tension>
   </Output>

</Add_equation>

</svMultiPhysicsFile>


